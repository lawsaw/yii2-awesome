  "description": "Всякие интересности для yii2",
  "type": "yii2-extension",
  "keywords": ["yii2","extension"],
  "license": "MIT",
  "authors": [
    {
      "name": "lawsaw",
      "email": "lawdrel@gmail.com"
    }
  ],
  "require": {
    "yiisoft/yii2": "~2.0.0",
    "omgdef/yii2-multilingual-behavior": "^2.1",
    "kartik-v/yii2-widget-select2": "@dev",
    "kartik-v/yii2-widget-fileinput": "1.0",
    "kartik-v/yii2-widget-datetimepicker": "*",
    "yiisoft/yii2-imagine": "*",
    "spatie/pdf-to-image": "^1.2",
    "true/punycode": "~2.0"
